{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "function": "upload"
      },
      {
        "source": "/**",
        "function": "app"
      }
    ]
  }
}
